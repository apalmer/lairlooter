  "storage": {
    "dynllmaps": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "llMapsFunc": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "dynllmaps",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
